{
  "title": "GitHub Runners",
  "description": "GitHub Actions self-hosted runner orchestration - Local (ai-lab), VPS, Cloud",
  "schemaVersion": 38,
  "version": 1,
  "uid": "github-runners",
  "tags": ["github", "runners", "ci-cd", "actions", "orchestration"],
  "refresh": "10s",
  "panels": [
    {
      "type": "row",
      "title": "Runner Status Overview",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Runners Online",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(github_runner_up)", "legendFormat": "Online" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 2 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Runners Busy",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(github_runner_busy)", "legendFormat": "Busy" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Total Queue Depth",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(github_runner_queue_depth)", "legendFormat": "Queued" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 15 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Jobs Completed (24h)",
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(increase(github_runner_jobs_total{status=\"success\"}[24h]))", "legendFormat": "Completed" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              { "color": "blue", "value": 0 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "API Rate Limit Remaining",
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "github_runner_github_api_rate_limit", "legendFormat": "Remaining" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 100 },
              { "color": "green", "value": 500 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Jobs Failed (24h)",
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "sum(increase(github_runner_jobs_total{status=\"failed\"}[24h]))", "legendFormat": "Failed" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Runner Details",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "table",
      "title": "Runner Status Table",
      "gridPos": { "x": 0, "y": 6, "w": 24, "h": 6 },
      "targets": [
        { "expr": "github_runner_up", "legendFormat": "{{runner}}", "format": "table", "instant": true },
        { "expr": "github_runner_busy", "legendFormat": "{{runner}}", "format": "table", "instant": true },
        { "expr": "github_runner_queue_depth", "legendFormat": "{{runner}}", "format": "table", "instant": true },
        { "expr": "github_runner_cpu_usage_percent", "legendFormat": "{{runner}}", "format": "table", "instant": true },
        { "expr": "github_runner_memory_bytes / 1024 / 1024 / 1024", "legendFormat": "{{runner}}", "format": "table", "instant": true },
        { "expr": "github_runner_disk_bytes / 1024 / 1024 / 1024", "legendFormat": "{{runner}}", "format": "table", "instant": true }
      ],
      "transformations": [
        { "id": "merge" },
        { "id": "organize", "options": { "excludeByName": { "__name__": true, "job": true, "instance": true }, "indexByName": { "runner": 0 }, "renameByName": { "runner": "Runner", "Value #github_runner_up": "Online", "Value #github_runner_busy": "Busy", "Value #github_runner_queue_depth": "Queue", "Value #github_runner_cpu_usage_percent": "CPU %", "Value #github_runner_memory_bytes": "Memory GB", "Value #github_runner_disk_bytes": "Disk GB" } } }
      ]
    },
    {
      "type": "row",
      "title": "Job Throughput",
      "gridPos": { "x": 0, "y": 12, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Job Completion Rate",
      "gridPos": { "x": 0, "y": 13, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(github_runner_jobs_total{status=\"success\"}[5m])) by (runner)", "legendFormat": "{{runner}} - success" },
        { "expr": "sum(rate(github_runner_jobs_total{status=\"failed\"}[5m])) by (runner)", "legendFormat": "{{runner}} - failed" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "fillOpacity": 30, "lineWidth": 2 } } }
    },
    {
      "type": "timeseries",
      "title": "Queue Depth Over Time",
      "gridPos": { "x": 12, "y": 13, "w": 12, "h": 8 },
      "targets": [
        { "expr": "github_runner_queue_depth", "legendFormat": "{{runner}} ({{repository}})" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "fillOpacity": 30, "lineWidth": 2 },
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Job Duration",
      "gridPos": { "x": 0, "y": 21, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Job Duration Percentiles",
      "gridPos": { "x": 0, "y": 22, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(github_runner_job_duration_seconds_bucket[5m])) by (runner, le))", "legendFormat": "{{runner}} - P50" },
        { "expr": "histogram_quantile(0.95, sum(rate(github_runner_job_duration_seconds_bucket[5m])) by (runner, le))", "legendFormat": "{{runner}} - P95" },
        { "expr": "histogram_quantile(0.99, sum(rate(github_runner_job_duration_seconds_bucket[5m])) by (runner, le))", "legendFormat": "{{runner}} - P99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "fillOpacity": 10, "lineWidth": 2 } } }
    },
    {
      "type": "timeseries",
      "title": "Job Duration by Workload Type",
      "gridPos": { "x": 12, "y": 22, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(github_runner_job_duration_seconds_bucket[5m])) by (type, le))", "legendFormat": "{{type}} - P95" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "fillOpacity": 20, "lineWidth": 2 } } }
    },
    {
      "type": "row",
      "title": "Resource Usage",
      "gridPos": { "x": 0, "y": 30, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "CPU Usage by Runner",
      "gridPos": { "x": 0, "y": 31, "w": 8, "h": 8 },
      "targets": [
        { "expr": "github_runner_cpu_usage_percent", "legendFormat": "{{runner}}" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "custom": { "fillOpacity": 30 },
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Memory Usage by Runner",
      "gridPos": { "x": 8, "y": 31, "w": 8, "h": 8 },
      "targets": [
        { "expr": "github_runner_memory_bytes / 1024 / 1024 / 1024", "legendFormat": "{{runner}}" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "fillOpacity": 30 },
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 8 },
              { "color": "red", "value": 14 }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Disk Usage by Runner",
      "gridPos": { "x": 16, "y": 31, "w": 8, "h": 8 },
      "targets": [
        { "expr": "github_runner_disk_bytes / 1024 / 1024 / 1024", "legendFormat": "{{runner}}" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "fillOpacity": 30 },
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Jobs by Type",
      "gridPos": { "x": 0, "y": 39, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Job Distribution by Type",
      "gridPos": { "x": 0, "y": 40, "w": 24, "h": 6 },
      "targets": [
        { "expr": "sum(rate(github_runner_jobs_total[5m])) by (type)", "legendFormat": "{{type}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "fillOpacity": 30 } } }
    },
    {
      "type": "piechart",
      "title": "Jobs by Type (24h)",
      "gridPos": { "x": 0, "y": 46, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(increase(github_runner_jobs_total[24h])) by (type)", "legendFormat": "{{type}}" }
      ],
      "options": { "pieType": "pie", "displayLabels": ["name", "value", "percent"] }
    },
    {
      "type": "piechart",
      "title": "Jobs by Status (24h)",
      "gridPos": { "x": 12, "y": 46, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(increase(github_runner_jobs_total[24h])) by (status)", "legendFormat": "{{status}}" }
      ],
      "options": { "pieType": "pie", "displayLabels": ["name", "value", "percent"] },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "success": { "color": "green" } } },
            { "type": "value", "options": { "failed": { "color": "red" } } },
            { "type": "value", "options": { "cancelled": { "color": "yellow" } } }
          ]
        }
      }
    }
  ],
  "time": { "from": "now-6h", "to": "now" }
}
