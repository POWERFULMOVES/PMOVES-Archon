{
  "title": "Render Webhook",
  "description": "ComfyUI render callback handler - webhook authentication, Supabase operations, and studio board management",
  "schemaVersion": 38,
  "version": 1,
  "uid": "render-webhook",
  "tags": ["webhook", "comfyui", "render", "supabase"],
  "refresh": "10s",
  "panels": [
    {
      "type": "row",
      "title": "Service Overview",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Service Status",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "up{job=\"render-webhook\"}", "legendFormat": "Status" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" } } },
            { "type": "value", "options": { "1": { "text": "UP", "color": "green" } } }
          ]
        }
      }
    },
    {
      "type": "stat",
      "title": "Total Webhooks",
      "gridPos": { "x": 4, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "sum(render_webhook_requests_total)", "legendFormat": "Webhooks" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "blue", "value": 0 }] } } }
    },
    {
      "type": "stat",
      "title": "Request Rate",
      "gridPos": { "x": 9, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "sum(rate(render_webhook_requests_total[5m]))", "legendFormat": "Rate" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps", "decimals": 2 } }
    },
    {
      "type": "stat",
      "title": "P95 Latency",
      "gridPos": { "x": 14, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(render_webhook_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P95" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] } } }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "gridPos": { "x": 19, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "sum(rate(render_webhook_requests_total{status=~\"4..|5..\"}[5m]))", "legendFormat": "Errors" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps", "decimals": 2, "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 0.1 }, { "color": "red", "value": 1 }] } } }
    },
    {
      "type": "row",
      "title": "Request Metrics",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Request Rate by Status",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(render_webhook_requests_total{status=\"success\"}[5m]))", "legendFormat": "Success (2xx)" },
        { "expr": "sum(rate(render_webhook_requests_total{status=\"error\"}[5m]))", "legendFormat": "Error (4xx/5xx)" },
        { "expr": "sum(rate(render_webhook_requests_total{status=\"unauthorized\"}[5m]))", "legendFormat": "Unauthorized (401)" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "drawStyle": "line", "fillOpacity": 30 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Success (2xx)" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Error (4xx/5xx)" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Unauthorized (401)" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Request Latency Percentiles",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(render_webhook_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P50" },
        { "expr": "histogram_quantile(0.95, sum(rate(render_webhook_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P95" },
        { "expr": "histogram_quantile(0.99, sum(rate(render_webhook_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "fillOpacity": 10 } } }
    },
    {
      "type": "row",
      "title": "Supabase Operations",
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Supabase Operations by Type",
      "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(render_webhook_supa_operations_total[5m])) by (operation)", "legendFormat": "{{operation}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "timeseries",
      "title": "Supabase Operations by Table",
      "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(render_webhook_supa_operations_total[5m])) by (table)", "legendFormat": "{{table}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "row",
      "title": "Error Analysis",
      "gridPos": { "x": 0, "y": 23, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Error Rate Breakdown",
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 6 },
      "targets": [
        { "expr": "sum(rate(render_webhook_requests_total{status=\"error\"}[5m]))", "legendFormat": "Errors" },
        { "expr": "sum(rate(render_webhook_requests_total{status=\"unauthorized\"}[5m]))", "legendFormat": "Unauthorized" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "drawStyle": "line", "fillOpacity": 30 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Errors" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Unauthorized" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "type": "stat",
      "title": "Total Errors",
      "gridPos": { "x": 12, "y": 24, "w": 6, "h": 6 },
      "targets": [{ "expr": "sum(render_webhook_requests_total{status=\"error\"})", "legendFormat": "Errors" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 10 }] } } }
    },
    {
      "type": "stat",
      "title": "Total Unauthorized",
      "gridPos": { "x": 18, "y": 24, "w": 6, "h": 6 },
      "targets": [{ "expr": "sum(render_webhook_requests_total{status=\"unauthorized\"})", "legendFormat": "Unauthorized" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] } } }
    }
  ],
  "time": { "from": "now-1h", "to": "now" }
}
