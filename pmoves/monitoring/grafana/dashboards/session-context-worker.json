{
  "title": "Session Context Worker",
  "description": "Session context management - NATS message processing, context tracking, and knowledge base upserts",
  "schemaVersion": 38,
  "version": 1,
  "uid": "session-context-worker",
  "tags": ["session", "context", "nats", "worker"],
  "refresh": "10s",
  "panels": [
    {
      "type": "row",
      "title": "Service Overview",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Service Status",
      "gridPos": { "x": 0, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "up{job=\"session-context-worker\"}", "legendFormat": "Status" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" } } },
            { "type": "value", "options": { "1": { "text": "UP", "color": "green" } } }
          ]
        }
      }
    },
    {
      "type": "stat",
      "title": "Total Messages",
      "gridPos": { "x": 5, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "sum(session_context_worker_messages_received_total)", "legendFormat": "Messages" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "blue", "value": 0 }] } } }
    },
    {
      "type": "stat",
      "title": "Processing Rate",
      "gridPos": { "x": 10, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "sum(rate(session_context_worker_messages_processed_total[5m]))", "legendFormat": "Rate" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "ops", "decimals": 2 } }
    },
    {
      "type": "stat",
      "title": "P95 Latency",
      "gridPos": { "x": 15, "y": 1, "w": 4, "h": 4 },
      "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(session_context_worker_processing_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P95" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3, "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 2 }] } } }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "gridPos": { "x": 19, "y": 1, "w": 5, "h": 4 },
      "targets": [{ "expr": "sum(rate(session_context_worker_messages_failed_total[5m]))", "legendFormat": "Errors" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "ops", "decimals": 2, "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 0.1 }, { "color": "red", "value": 1 }] } } }
    },
    {
      "type": "row",
      "title": "Message Metrics",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Messages Received by Subject",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_messages_received_total[5m])) by (subject)", "legendFormat": "{{subject}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "timeseries",
      "title": "Messages Processed by Context Type",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_messages_processed_total[5m])) by (context_type)", "legendFormat": "{{context_type}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "timeseries",
      "title": "Processing Rate Trend",
      "gridPos": { "x": 0, "y": 14, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_messages_processed_total[5m]))", "legendFormat": "Processed" },
        { "expr": "sum(rate(session_context_worker_messages_received_total[5m]))", "legendFormat": "Received" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Processed" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Received" }, "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "KB Upserts Published by Namespace",
      "gridPos": { "x": 12, "y": 14, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_kb_upserts_published_total[5m])) by (namespace)", "legendFormat": "{{namespace}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "row",
      "title": "Latency Analysis",
      "gridPos": { "x": 0, "y": 22, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Processing Duration Percentiles",
      "gridPos": { "x": 0, "y": 23, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(session_context_worker_processing_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P50" },
        { "expr": "histogram_quantile(0.95, sum(rate(session_context_worker_processing_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P95" },
        { "expr": "histogram_quantile(0.99, sum(rate(session_context_worker_processing_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "fillOpacity": 10 } } }
    },
    {
      "type": "timeseries",
      "title": "Processing Duration by Context Type",
      "gridPos": { "x": 12, "y": 23, "w": 12, "h": 8 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(session_context_worker_processing_duration_seconds_bucket[5m])) by (le, context_type))", "legendFormat": "P95 {{context_type}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "heatmap",
      "title": "Processing Duration Distribution",
      "gridPos": { "x": 0, "y": 31, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_processing_duration_seconds_bucket[5m])) by (le)", "legendFormat": "{{le}}" }
      ],
      "options": { "calculate": false, "cellGap": 2, "cellRadius": 0, "cellValues": {} },
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "timeseries",
      "title": "Average Processing Duration",
      "gridPos": { "x": 12, "y": 31, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_processing_duration_seconds_sum[5m])) / sum(rate(session_context_worker_processing_duration_seconds_count[5m]))", "legendFormat": "Average" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "drawStyle": "line", "fillOpacity": 30 } } }
    },
    {
      "type": "row",
      "title": "Error Analysis",
      "gridPos": { "x": 0, "y": 39, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Failed Messages by Error Type",
      "gridPos": { "x": 0, "y": 40, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_messages_failed_total[5m])) by (error_type)", "legendFormat": "{{error_type}}" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "validation_error" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "nats_error" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "supabase_error" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate Trend",
      "gridPos": { "x": 12, "y": 40, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(session_context_worker_messages_failed_total[5m]))", "legendFormat": "Failed" },
        { "expr": "sum(rate(session_context_worker_messages_processed_total[5m]))", "legendFormat": "Processed" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "fillOpacity": 30 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Failed" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Processed" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "type": "stat",
      "title": "Total Errors",
      "gridPos": { "x": 0, "y": 48, "w": 6, "h": 6 },
      "targets": [{ "expr": "sum(session_context_worker_messages_failed_total)", "legendFormat": "Errors" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 10 }] } } }
    },
    {
      "type": "stat",
      "title": "Success Rate",
      "gridPos": { "x": 6, "y": 48, "w": 6, "h": 6 },
      "targets": [{ "expr": "sum(rate(session_context_worker_messages_processed_total[5m])) / (sum(rate(session_context_worker_messages_received_total[5m])) + 0.01) * 100", "legendFormat": "Success %" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "percent", "decimals": 1, "thresholds": { "steps": [{ "color": "red", "value": 0 }, { "color": "yellow", "value": 95 }, { "color": "green", "value": 99 }] } } }
    },
    {
      "type": "stat",
      "title": "KB Upserts Total",
      "gridPos": { "x": 12, "y": 48, "w": 6, "h": 6 },
      "targets": [{ "expr": "sum(session_context_worker_kb_upserts_published_total)", "legendFormat": "Upserts" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "thresholds": { "steps": [{ "color": "blue", "value": 0 }] } } }
    },
    {
      "type": "stat",
      "title": "Avg Processing Time",
      "gridPos": { "x": 18, "y": 48, "w": 6, "h": 6 },
      "targets": [{ "expr": "sum(rate(session_context_worker_processing_duration_seconds_sum[5m])) / sum(rate(session_context_worker_processing_duration_seconds_count[5m]))", "legendFormat": "Avg" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3, "thresholds": { "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 2 }] } } }
    }
  ],
  "time": { "from": "now-1h", "to": "now" }
}
