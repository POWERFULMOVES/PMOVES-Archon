# PMOVES v5 .env.example
PMOVES_NAMESPACE=darkxside
PMOVES_DOMAIN=pmoves.local

# N8N
N8N_HOST=n8n.${PMOVES_DOMAIN}
N8N_PORT=5678
N8N_PROTOCOL=http

# Neo4j
NEO4J_PASSWORD=change_me_neo4j
NEO4J_URL=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_DATABASE=neo4j

# MinIO
MINIO_ROOT_USER=pmoves
MINIO_ROOT_PASSWORD=change_me_minio
MINIO_ENDPOINT=minio:9000
MINIO_USE_SSL=false
MINIO_BUCKET_COMFYUI=pmoves-comfyui
MINIO_EXTERNAL_ENDPOINT=http://localhost:9000
PRESIGN_EXPIRES_HOURS=24

# Qdrant
QDRANT__SERVICE__GRPC_PORT=6334
QDRANT__SERVICE__HTTP_PORT=6333

# Agents
AGENT_ZERO_PORT=8080
ARCHON_PORT=8090

# Ollama / ComfyUI
OLLAMA_PORT=11434
COMFYUI_PORT=8188

# Supabase CE
SUPABASE_DB_PASSWORD=change_me_pg
SUPABASE_JWT_SECRET=super_secret_jwt_change_me
SUPABASE_ANON_KEY=anon_key_change_me
SUPABASE_SERVICE_ROLE_KEY=service_role_key_change_me

# Jellyfin + publisher
JELLYFIN_URL=http://jellyfin:8096
JELLYFIN_API_KEY=change_me_jellyfin_key
MEDIA_LIBRARY_PATH=/library/images
PUBLISH_LIBRARY_NAME=Images
